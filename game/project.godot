; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Node",
"class": "AreaItemManage",
"language": "GDScript",
"path": "res://script/start/area_item_manage.gd"
} ]
_global_script_class_icons={
"AreaItemManage": ""
}

[application]

config/name="sunset"
run/main_scene="res://scene/splash/splash_screen.tscn"
boot_splash/image="res://texture/splash/blank.png"
config/icon="res://icon.png"

[autoload]

TextTransitionSettings="*res://addons/teeb.text_transitions/TextTransitionSettings.gd"
StatusUtils="*res://addons/event_chain/src/common/status_util.gd"
SignalUtils="*res://addons/event_chain/src/common/signal_util.gd"
EventChainGraphManage="*res://addons/event_chain/src/core/graph_manage.gd"
EventChainSignalManage="*res://addons/event_chain/src/core/signal_manage.gd"
SceneMgr="*res://script/common/scene_mgr.gd"
TalkMgr="*res://script/taking/talk_mgr.gd"
ItemManage="*res://script/common/item_manage.gd"
Configs="*res://data/gdscript/configs.gd"
GlobalStatusMgr="*res://script/common/global_status_mgr.gd"

[display]

window/size/width=1280
window/size/height=720
window/size/always_on_top=true
window/size/test_width=1280
window/size/test_height=720
window/dpi/allow_hidpi=true

[editor_plugins]

enabled=PoolStringArray( "zylann.translation_editor" )

[event_chain_graph]

generation/enable_multithreading=true
generation/delay_before_generation=75
user_interface/inline_vector_fields=false
user_interface/group_nodes_by_type=false

[rendering]

quality/2d/use_pixel_snap=true
environment/default_environment="res://default_env.tres"
